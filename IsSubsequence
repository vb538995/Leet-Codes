// LeetCode : 392 IsSubsequence


CODE : 
class Solution {
public:
    bool isSubsequence(string s, string t) {
        int i;
        int j;
        int x;
        i=j=x=0;
        for(i=0;i<t.size();i++){
            if(s[j]==t[i]){
                j++;
                x++;
            }
            else{
                continue;
            }
        }
        if(x==s.size()){
            return true;
        }
        return false;
    }
};
